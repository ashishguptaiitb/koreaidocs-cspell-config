// cSpell Settings
{
  "enabledFileTypes": [
    "txt",
    "json",
    "!svg",
    "markdown",
    "!png",
    "yml"
  ],

  "version": "0.2",
  "language": "en",

	//List of ignored words. Ignored means that these words aren't part of a dictionary and aren't considered errors--just neutral words.

	"import": [
		"./config/ignore-words.json",
	
		"./config/kore-custom-rejected-words.json"
	
	],
  "dictionaryDefinitions": [
    {
      "name": "allowedWords",
      "path": "./config/kore-custom-dict.txt",
      "addWords": false
    }
    // Do NOT define a "flaggedWords" dictionary below. Otherwise, Cspell will allow those words instead of flagging.
  ],
  "dictionaries": [
    "allowedWords"
  ]
}
